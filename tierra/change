dec_c  ->  decC
inc_a  ->  incA
inc_b  ->  incB
inc_c  ->  incC
jmp    ->  jmpo
movab  ->  movBA
movcd  ->  movDC
popax  ->  popA
popbx  ->  popB
popcx  ->  popC
popdx  ->  popD
pushax ->  pushA
pushbx ->  pushB
pushcx ->  pushC
pushdx ->  pushD
sub_ab ->  subCAB
sub_ac ->  subAAC
zerod  ->  zeroD
ax     ->  A
bx     ->  B
cx     ->  C
dx     ->  D
